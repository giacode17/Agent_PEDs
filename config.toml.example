# Example configuration file for peds-post-discharge-agent
# Copy this to config.toml and replace values with your actual credentials

[cli.options]
  stream = false
  payload_path = ""

[deployment.online.parameters]
  model_id = "ibm/granite-3-3-8b-instruct"  # underlying model of WatsonxChat
  url = "https://us-south.ml.cloud.ibm.com"
  space_id = "YOUR_SPACE_ID_HERE"  # Get from IBM Watsonx

[deployment.software_specification]
  name = "runtime-24.1-py3.11"
  base_sw_spec = "runtime-24.1-py3.11"

[mlflow]
enabled = false           # set true when ready
tracking_uri = "file:./mlruns"  # or remote MLflow server
experiment_name = "peds_post_discharge_agent"
